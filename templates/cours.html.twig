{% extends 'base.html.twig' %}

{% block body %}
	<h1 class="text-center">Liste des cours</h1>
	<table class="table">
		<thead>
			<tr>
				<th scope="col">titre</th>
				<th scope="col">description</th>
			</tr>
		</thead>
		<tbody>

			{% for cours in cours %}
				<tr>
					
					<td>{{cours.titre}}</td>
					<td>{{cours.description}}</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>

{% endblock %}
